{
  "name": "menu",
  "displayName": "helloworld-menu",
  "description": "using menu launch commands",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "menu.helloWorld",
        "title": "hello world" 
      },
      {
        "command": "menu.command1",
        "title": "命令1" 
      },
      {
        "command": "menu.command2",
        "title": "命令2" 
      },
      {
        "command": "menu.command3",
        "title": "命令3" 
      },
      {
        "command": "menu.command4",
        "title": "命令4" 
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "test",
          "group": "navigation",
          "title": "子菜单名称"
        },
        {
          "submenu": "test1",
          "group": "navigation",
          "title": "子菜单名称1"
        }
      ],
      "test": [
        {
          "command": "menu.command1",
          "group": "group1"
        },
        {
          "command": "menu.helloWorld",
          "group": "group1"
        },
        {
          "command": "menu.command2",
          "group": "group2"
        }
      ],
      "test1": [
        {
          "command": "menu.command3",
          "group": "group1"
        },
        {
          "command": "menu.command4",
          "group": "group2"
        }
      ]
    },
    "submenus": [
      {
        "id": "test",
        "label": "菜单"
      },
      {
        "id": "test1",
        "label": "菜单1"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
