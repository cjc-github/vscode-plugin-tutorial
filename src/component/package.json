{
  "name": "component",
  "displayName": "common_components",
  "description": "common components of vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "My Extension Configuration",
      "type": "object",
      "properties": {
        "myextension.int_var": {
          "type": "integer",
          "default": 1,
          "minimum": 1,
          "maximum": 50,
          "description": "Defines the number allowed (1-50), recommended value <= 10."
        }
      }
    },
    "commands": [
      {
        "command": "component.helloWorld",
        "title": "执行命令"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "test1",
          "group": "navigation",
          "title": "菜单"
        }
      ],
      "test1": [
        {
          "command": "component.helloWorld",
          "group": "group1"
        }
      ]
    },
    "submenus": [
      {
        "id": "test1",
        "label": "命令1"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
